# 邮件服务配置
APP_NAME=Email Service
DEBUG=false
PORT=8025

# API Key：MathStudyPlatform 调用时需携带 X-API-Key
API_KEY=your-secret-api-key-change-in-production

# SMTP 配置（以 QQ 邮箱示例，实际替换为你的 SMTP）
SMTP_HOST=smtp.qq.com
SMTP_PORT=587
SMTP_USE_TLS=false
SMTP_START_TLS=true
SMTP_USER=your-email@qq.com
SMTP_PASSWORD=your-smtp-auth-code
SMTP_FROM_EMAIL=your-email@qq.com
SMTP_FROM_NAME=MathStudyPlatform
